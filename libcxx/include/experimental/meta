                                           R &&args) {
  vector<info> targs(from_range, args);
  targs.insert(targs.begin(), type);
  targs.insert(targs.begin(), type_result);

  return extract<bool>(substitute(LIFT(is_nothrow_invocable_r_v), targs));
}

